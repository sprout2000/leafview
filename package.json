{
  "name": "lessview",
  "productName": "LessView",
  "version": "0.1.12",
  "main": "dist/main.js",
  "description": "An image viewer for minimalists",
  "author": {
    "name": "sprout2000",
    "url": "https://github.com/sprout2000"
  },
  "homepage": "https://github.com/sprout2000/lessview#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/lessview.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/lessview/issues"
  },
  "keywords": [
    "image-viewer",
    "cross-platform",
    "electron"
  ],
  "engines": {
    "node": ">=10.14",
    "npm": "please_use_yarn_instead"
  },
  "os": [
    "win32",
    "darwin"
  ],
  "scripts": {
    "start": "run-s clean build serve",
    "clean": "rimraf dist release",
    "build": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack --progress",
    "serve": "electron .",
    "release": "node scripts/build.js",
    "prepackage": "run-s clean build:prod",
    "package": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false node scripts/build-unsign.js",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:push": "git push --follow-tags origin master"
  },
  "dependencies": {
    "electron-log": "^4.2.0",
    "electron-updater": "^4.3.1",
    "electron-window-state": "^5.0.3",
    "howler": "^2.2.0",
    "i18next": "^19.4.5",
    "leaflet": "^1.6.0",
    "mime-types": "^2.1.27",
    "natsort": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-resize-detector": "^4.2.3"
  },
  "devDependencies": {
    "@types/electron-load-devtool": "^1.2.0",
    "@types/howler": "^2.1.2",
    "@types/leaflet": "^1.5.12",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-resize-detector": "^4.2.0",
    "@types/styled-components": "^5.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^8.3.0",
    "electron-builder": "^22.6.1",
    "electron-load-devtool": "^1.2.0",
    "electron-notarize": "^0.3.0",
    "html-webpack-plugin": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "styled-components": "^5.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "optionalDependencies": {
    "@types/webpack": "^4.41.13",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.3",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0"
  }
}
