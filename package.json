{
  "name": "leafview",
  "productName": "LeafView",
  "description": "A minimalist image viewer based on Leaflet.js and Electron.",
  "version": "2.0.5",
  "license": "MIT",
  "author": {
    "name": "sprout2000"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/leafview.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "predev": "rimraf dist",
    "dev": "run-p dev:*",
    "dev:esbuild": "cross-env NODE_ENV=\"development\" ts-node ./esbuild.ts",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && cross-env NODE_ENV=\"development\" electron .",
    "build": "cross-env NODE_ENV=\"production\" ts-node ./esbuild.ts",
    "package": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=\"false\" ts-node scripts/build.ts",
    "release": "ts-node scripts/build.ts",
    "prebuild": "npm r --no-save @typescript-eslint/eslint-plugin @typescript-eslint/parser electron-reload eslint eslint-config-prettier eslint-plugin-react eslint-plugin-react-hooks npm-run-all prettier rimraf wait-on"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.2.1",
    "i18next": "^21.9.2",
    "leaflet": "^1.9.1",
    "mime-types": "^2.1.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@craftamap/esbuild-plugin-html": "^0.4.0",
    "@sprout2000/esbuild-copy-plugin": "^1.1.1",
    "@types/leaflet": "^1.8.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "electron": "^21.0.0",
    "electron-builder": "^23.3.3",
    "electron-notarize": "^1.2.1",
    "electron-search-devtools": "^1.8.7",
    "esbuild": "^0.15.9",
    "esbuild-sass-plugin": "^2.3.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "wait-on": "^6.0.1"
  }
}
